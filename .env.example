# Google AI Studio (primary LLM)
GOOGLE_AI_API_KEY=your-google-ai-key-here
GOOGLE_AI_MODEL=gemini-2.5-pro-preview-05-06

# Qdrant (local vector DB for mem0)
QDRANT_HOST=localhost
QDRANT_PORT=6333

# Telegram Bot
TELEGRAM_BOT_TOKEN=your-bot-token-here

# Cloud quick-answer model (lighter model for simple queries)
CLOUD_FAST_MODEL=gemini-2.0-flash

# Embedding model (for future use)
EMBEDDING_MODEL=nomic-embed-text

# Scheduler
HEARTBEAT_INTERVAL_MINUTES=30
